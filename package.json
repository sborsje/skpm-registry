  },
  "devDependencies": {
    "aws-sdk": "^2.6.1",
    "chai": "^3.5.0",
    "injectr": "^0.5.1",
    "mocha": "^3.0.2",
    "nsp": "^2.6.1",
    "serverless": "^1.4.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
